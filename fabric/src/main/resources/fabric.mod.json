{
  "schemaVersion": 1,
  "id": "mobz",
  "version": "${version}",
  "name": "MobZ",
  "description": "More vanilla style mobs",
  "authors": [
    "Globox_Z",
    "Rikka0w0"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/mobz",
    "sources": "https://github.com/rikka0w0/MobZReborn",
    "issues": "https://github.com/rikka0w0/MobZReborn/issues"
  },
  "license": "GPLv3",
  "icon": "mobz_logo.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.mobz.fabric.DataGeneratorEntry"
    ],
    "main": [
      "net.mobz.fabric.FabricEntry"
    ],
    "modmenu": [
      "net.mobz.fabric.ModMenuIntegration"
    ],
    "client": [
      "net.mobz.fabric.FabricClientEntry"
    ]
  },
  "mixins": [
    "mobz.mixins.json"
  ],
  "accessWidener" : "mobz.accesswidener",
  "depends": {
    "minecraft": ">=${minecraft_version_min} <${minecraft_version_max}",
    "fabricloader": ">=${loader_version_min}",
    "fabric-api": ">=${fabric_version_min}",
    "cloth-config2": ">=${cloth_config_version_min}",
    "fabricshieldlib": ">=${fabric_shield_lib_version_min}"
  },
  "suggests": {
    "flamingo": "*"
  }
}
